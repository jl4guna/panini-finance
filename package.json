    "c8": "^9.1.0",
    "cookie": "^0.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "happy-dom": "^13.3.8",
