    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.5.0",
    "tiny-invariant": "^1.3.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
